@model IEnumerable<SCS.Models.Rates>
@{
    Layout = null;
}
<div class="moreInformationOrder" id="transport">
    <div class="form-group">
        <div class="col-md-10">
            <label>Наименование тарифа</label>
            @Html.DropDownList("RatesId", null, htmlAttributes: new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10 accessories">
            <label>Наименование аксессуара</label>

            @Html.DropDownList("AccessoriesId" , null, htmlAttributes: new { @class = "form-control" })

        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <label>Количество, шт.</label>
            <input type="number" id="countAccessories" name="countAccessories" class="form-control"
                   min="1" max="100" />
        </div>
    </div>
</div>