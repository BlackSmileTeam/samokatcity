@model IEnumerable<SCS.Models.Accessories>
@{
    Layout = null;
}
<div class="moreInformationOrder" id="transport">
    <div class="form-group">
        <div class="col-md-10">
            <label>Тариф</label>
            @Html.DropDownList("RatesId", ((IEnumerable<SelectListItem>)ViewData["Menu"]), "Выберете значение", htmlAttributes: new { @class = "Rate form-control", @onchange = "calculatePrice()" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10 accessories">
            <label>Наименование</label>
            @Html.DropDownList("AccessoriesId", null, htmlAttributes: new { @class = "form-control" })
        </div>
    </div>
    
    <div class="form-group">
        <div class="col-md-10">
        </div>
    </div>

</div>
